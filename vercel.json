{
  "version": 2,
  "builds": [
    {
      "src": "server/index.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/server/index.ts"
    }
  ],
  "functions": {
    "server/index.ts": {
      "includeFiles": [
        "server/**",
        "shared/**",
        "client/dist/**"
      ]
    }
  }
}
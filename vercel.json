{
  "version": 2,
  "builds": [
    {
      "src": "server/index.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server/index.ts"
    },
    {
      "src": "/(.*)",
      "use": "@vercel/static",
      "dest": "/client/dist/$1"
    }
  ],
  "functions": {
    "server/index.ts": {
      "includeFiles": "shared/**"
    }
  }
}